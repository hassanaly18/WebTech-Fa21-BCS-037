  <div class="row" id="featured">
    <div class="row" id="row1">
      <% featuredProducts.slice(0, 3).forEach(product => { %>
        <div class="col-md-4 col-sm-6">
          <div class="product-grid">
            <div class="product-image">
              <a href="/products/<%= product._id %>" class="image">
                <img class="pic-1" src="<%= product.imageUrl[0] %>">
                <img class="pic-2" src="<%= product.imageUrl[1] %>">
              </a>
              <a href="#" class="product-like-icon" data-tip="Add to Wishlist">
                <i class="far fa-heart"></i>
              </a>
              <ul class="product-links">
                <li><a href="#"><i class="fa fa-search"></i></a></li>
                <li><a href="#"><i class="fas fa-shopping-cart"></i></a></li>
                <li><a href="#"><i class="fa fa-random"></i></a></li>
              </ul>
            </div>
            <div class="product-content">
              <h3 class="title"><%= product.title %></h3>
              <div class="price">$<%= product.price %></div>
            </div>
          </div>
        </div>
      <% }) %>
    </div>
    <div class="row" id="row2">
      <% featuredProducts.slice(3, 5).forEach(product => { %>
        <div class="col-md-4 col-sm-6">
          <div class="product-grid">
            <div class="product-image">
              <a href="/products/<%= product._id %>" class="image">
                <img class="pic-1" src="<%= product.imageUrl[0] %>">
                <img class="pic-2" src="<%= product.imageUrl[1] %>">
              </a>
              <a href="#" class="product-like-icon" data-tip="Add to Wishlist">
                <i class="far fa-heart"></i>
              </a>
              <ul class="product-links">
                <li><a href="#"><i class="fa fa-search"></i></a></li>
                <li><a href="#"><i class="fas fa-shopping-cart"></i></a></li>
                <li><a href="#"><i class="fa fa-random"></i></a></li>
              </ul>
            </div>
            <div class="product-content">
              <h3 class="title"><%= product.title %></h3>
              <div class="price">$<%= product.price %></div>
            </div>
          </div>
        </div>
      <% }) %>
    </div>
  </div>
  