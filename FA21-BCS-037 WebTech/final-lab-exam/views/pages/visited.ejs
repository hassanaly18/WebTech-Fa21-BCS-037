<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Visited Products</title>
  <link rel="stylesheet" href="/path/to/your/css/styles.css">
</head>
<body>
  <div class="container">
    <h1>Visited Products</h1>
    <div class="row">
      <% visitedProducts.forEach(product => { %>
        <div class="col-md-3 col-sm-6">
          <div class="product-grid">
            <div class="product-image">
              <a href="/products/<%= product._id %>" class="image">
                <img class="pic-1" src="<%= product.imageUrl[0] %>">
                <img class="pic-2" src="<%= product.imageUrl[1] %>">
              </a>
              <a href="#" class="product-like-icon" data-tip="Add to Wishlist">
                <i class="far fa-heart"></i>
              </a>
              <ul class="product-links">
                <li><a href="#"><i class="fa fa-search"></i></a></li>
                <li><a href="#"><i class="fas fa-shopping-cart"></i></a></li>
                <li><a href="#"><i class="fa fa-random"></i></a></li>
              </ul>
            </div>
            <div class="product-content">
              <h3 class="title"><%= product.title %></h3>
              <div class="price">$<%= product.price %></div>
            </div>
          </div>
        </div>
      <% }) %>
    </div>
  </div>
</body>
</html>
